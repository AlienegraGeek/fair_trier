{
  "className": "Scaffold",
  "na": {
    "appBar": {
      "className": "AppBar",
      "na": {
        "title": {
          "className": "Text",
          "pa": [
            "^(_title)"
          ]
        }
      }
    },
    "body": {
      "className": "Center",
      "na": {
        "child": {
          "className": "Column",
          "na": {
            "mainAxisAlignment": "#(MainAxisAlignment.center)",
            "children": [
              {
                "className": "Sugar.ifEqualBool",
                "pa": [
                  "%(_countCanMod2)"
                ],
                "na": {
                  "falseValue": {
                    "className": "Image.asset",
                    "pa": [
                      "assets/image/img1.png"
                    ]
                  },
                  "trueValue": {
                    "className": "Image.asset",
                    "pa": [
                      "assets/image/img2.png"
                    ]
                  }
                }
              },
              {
                "className": "Padding",
                "na": {
                  "padding": {
                    "className": "EdgeInsets.only",
                    "na": {
                      "top": 20
                    }
                  },
                  "child": {
                    "className": "Text",
                    "pa": [
                      "#(_count = $_count)"
                    ]
                  }
                }
              },
              {
                "className": "Padding",
                "na": {
                  "padding": {
                    "className": "EdgeInsets.only",
                    "na": {
                      "top": 20
                    }
                  },
                  "child": {
                    "className": "Text",
                    "pa": [
                      "if _count % 2 == 1,  update image !"
                    ]
                  }
                }
              }
            ]
          }
        }
      }
    },
    "floatingActionButton": {
      "className": "Row",
      "na": {
        "mainAxisAlignment": "#(MainAxisAlignment.end)",
        "children": [
          {
            "className": "FloatingActionButton",
            "na": {
              "onPressed": "@(onTapText)",
              "child": {
                "className": "Icon",
                "pa": [
                  "#(Icons.add)"
                ]
              }
            }
          }
        ]
      }
    }
  },
  "methodMap": {}
}